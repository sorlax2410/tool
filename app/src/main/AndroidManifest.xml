<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mitdroid.kenshi.Main">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.kenshi.GUI.ScanningActivities.midDroidScreenActivity" />
        <activity android:name="com.kenshi.GUI.ScanningActivities.displayFormattedTargets" />
        <activity android:name="com.kenshi.GUI.ScanningActivities.displayTargets" />
        <activity android:name="com.kenshi.GUI.Wifi.WifiScannerActivity" />
        <activity android:name="com.kenshi.GUI.Wifi.SettingsActivity" />
        <activity android:name="com.kenshi.GUI.Wifi.ActionActivity" />
        <activity android:name="com.kenshi.Plugins.MITM_GUI.HijackerActivity" />
        <activity android:name="com.kenshi.Plugins.MITM_GUI.HijackerWebViewActivity" />
        <activity android:name="com.kenshi.Plugins.MITM_GUI.SnifferActivity"></activity>
    </application>

</manifest>